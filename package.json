{
  "name": "@discue/ui-components",
  "version": "0.2.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build-lib && npm run build-types",
    "build-types": "vue-tsc --declaration --emitDeclarationOnly",
    "build-lib": "vue-cli-service build --target lib --name ui src/index.ts && rm dist/demo.html",
    "test:unit": "vue-cli-service test:unit --mode production",
    "test:e2e": "vue-cli-service test:e2e --mode production",
    "lint": "vue-cli-service lint",
    "release": "standard-version && git push --follow-tags origin",
    "release-minor": "standard-version --release-as minor && git push --follow-tags origin"
  },
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/ui.common.js",
  "exports": {
    ".": {
      "import": "./dist/ui.common.js",
      "require": "./dist/ui.common.js"
    }
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "core-js": "^3.22.3",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "^5.0.4",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^8.14.0",
    "eslint-plugin-vue": "^8.0.3",
    "standard-version": "^9.3.2",
    "ts-debounce": "^4.0.0",
    "vue-tsc": "^0.34.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
